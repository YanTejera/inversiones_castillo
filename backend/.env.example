# Archivo de ejemplo para variables de entorno locales
# Copia este archivo como .env y personaliza los valores

DEBUG=True > .env
SECRET_KEY=django-insecure-local-development-key >> .env
ALLOWED_HOSTS=localhost,127.0.0.1 >> .env
CORS_ALLOW_ALL_ORIGINS=True >> .env
USE_SQLITE=True >> .env

# Para desarrollo local, estas variables no son necesarias:
# DATABASE_URL=
# FRONTEND_URL=